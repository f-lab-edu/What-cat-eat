[flake8]
exclude =
    .git,
    .gitignore,
    *.pot,
    *.py[co],
    __pycache__,
    env,
    .env
    alembic/migrations/*,


ignore =
    E121,
    E126,
    E127,
    E128,
    E203,
    E231,
    E241,
    E251,
    E501,
    E741,
    W291,
    W292,
    W293,
    W391,
    W503,
    B950,

max-line-length = 200
